<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>风尚物流</title>
   	<link rel="stylesheet" type="text/css" href="${css}/style.css" />
    <link rel="stylesheet" href="${css}/base.css"> <!--初始化文件-->
    
    <script type="text/javascript" src='../resources/include/js/jquery-1.8.2.js' charset='utf-8'></script>
    <script type="text/javascript" src='../resources/include/js/jquery.validate.js' charset='utf-8'></script>
    <script type="text/javascript" src='../resources/include/js/jquery.min.js' charset='utf-8'></script>
</head>
<body>

    <div class="someinformation">
        <div class="pagemain">
            <div class="big2">
                <p class="tit">配送点&nbsp/&nbsp配送点添加、编辑</p>
                <hr>
                <form>
                    <table>
                        <tr>
                            <td style="width: 60%;text-align: center">
                                配送点名称
                            </td>
                            <td style="width: 40%">
                                <input type="text" id="pointname" name="pointname">
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align: center">
                                配送点邮编
                            </td>
                            <td >
                                <input type="text" id="pointstamp" name="pointstamp">
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align: center">
                                配送点地址
                            </td>
                            <td >
                            	<select id = "provice"></select>
                            </td>
                            <td>
                            	<select id = "city"></select>
                            </td>
                            <td>
                            	<select id = "district"></select>	
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align: center">
                                配送点电话
                            </td>
                            <td >
                                <input type="text" id="pointphone" name="pointphone">
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align: center">
                                收入
                            </td>
                            <td >
                                <input type="text" id="icome" name="icome">
                            </td>
                        </tr>
                        <tr>
                            <td>
                            </td>
                            <td style="float: right">
                                <input type="button" id="submit" value="提交">
                                <input type="reset" value="重置">
                            </td>
                        </tr>
                    </table>
                </form>
            </div>
    </div>
</div>
    <script type="text/javascript" src='../resources/include/js/common.js' charset='utf-8'></script>
	<script type="text/javascript" src='../resources/include/js/city-set.js' charset='utf-8'></script>
	<script type="text/javascript" src='../resources/include/js/city-data.js' charset='utf-8'></script>
	<script type="text/javascript" src='../resources/include/js/point_add.js' charset='utf-8'></script>
</body>
</html>